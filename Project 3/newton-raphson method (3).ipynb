   "source": [
    "#Cube root\n",
    "#Find the cube root of x**3 = 25\n",
    "epsilon = 0.01\n",
    "numguess = 0\n",
    "k = 25.0\n",
    "guess = k/2\n",
    "while abs(guess**3-k) >= epsilon:\n",
    "    guess = ((k/guess**2)+(2*guess))/3\n",
    "    numguess = numguess+1\n",
    "print('Cube root of',k ,'is about',guess)\n",
    "print('numguess = ',numguess)"
   ]
